{
  "manifestVersion": 1,
  "id": "cabinet-maker",
  "name": "Make Cabinet File Task",
  "version": "1.0.0",
  "publisher": "KMM",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Tool for creating cabinet files. Includes one build/release task.",
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/filing-cabinet.png"
  },
  "files": [
    {
      "path": "buildAndReleaseTask"
    }
  ],
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "buildAndReleaseTask"
      }
    }
  ]
}